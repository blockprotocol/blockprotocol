{
  "$schema": "https://turborepo.org/schema.json",
  "pipeline": {
    "build": {
      "outputs": [
        "dist/**",
        ".next/**",
        "public/blocks/**",
        "public/schemas/**"
      ],
      "dependsOn": ["codegen", "^build"]
    },
    "codegen": {
      "outputs": ["blocks-data.json", "site-map.json"]
    },
    "fix:eslint": {
      "cache": false
    },
    "lint:eslint": {
      "dependsOn": ["^build"],
      "outputs": []
    },
    "lint:tsc": {
      "dependsOn": ["^build"],
      "outputs": []
    }
  }
}
