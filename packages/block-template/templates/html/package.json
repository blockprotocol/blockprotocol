{
  "name": "html-block",
  "version": "0.0.0",
  "description": "Block Protocol block template for an html-based block",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "homepage": "https://blockprotocol.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "packages/block-template"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "scripts": {
    "build": "parcel build && cp src/block-schema.json ./dist/block-schema.json && cp src/block-metadata.json ./dist/block-metadata.json && cp -R public dist/public",
    "dev": "echo \"Serving at http://localhost:63212\"; http-server -p 63212 ./dev",
    "lint:tsc": "echo \"Skipping: TypeScript not configured\"",
    "prepare": "rimraf ./dev/src; lnk -f ./src ./dev"
  },
  "dependencies": {},
  "devDependencies": {
    "http-server": "^14.1.1",
    "lnk-cli": "^1.0.1",
    "rimraf": "^3.0.2"
  }
}
