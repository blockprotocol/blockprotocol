{
  "name": "custom-element-block",
  "version": "0.0.0",
  "private": true,
  "description": "Block Protocol block template for a custom element-based block",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "homepage": "https://blockprotocol.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "packages/block-template"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "scripts": {
    "build": "node_modules/.bin/block-scripts build",
    "dev": "node_modules/.bin/block-scripts dev",
    "lint:tsc": "tsc --noEmit",
    "serve": "node_modules/.bin/block-scripts serve"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.12",
    "lit": "^2.2.5"
  },
  "devDependencies": {
    "block-scripts": "0.0.14",
    "mock-block-dock": "0.0.18",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "custom-element",
      "tagName": "my-block"
    },
    "displayName": "Template",
    "examples": [
      {
        "name": "World"
      }
    ],
    "icon": "public/omega.svg",
    "image": "public/block-preview.svg",
    "protocol": "0.2"
  }
}
