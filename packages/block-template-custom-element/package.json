{
  "name": "block-template-custom-element",
  "version": "0.1.8",
  "description": "Block Protocol block template for a custom element-based block",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "homepage": "https://blockprotocol.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "packages/block-template-custom-element"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "format": "prettier \"**/*\" --write --ignore-unknown",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.16",
    "lit": "^2.2.5"
  },
  "devDependencies": {
    "@types/react-dom": "^18.0.6",
    "block-scripts": "0.0.16",
    "mock-block-dock": "0.0.28",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "custom-element",
      "tagName": "my-block"
    },
    "displayName": "Template",
    "examples": [
      {
        "name": "World"
      }
    ],
    "icon": "public/omega.svg",
    "image": "public/block-preview.svg",
    "name": "block-template-custom-element",
    "protocol": "0.2"
  }
}
