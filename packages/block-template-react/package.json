{
  "name": "block-template-react",
  "version": "0.1.15",
  "description": "Block Protocol template for a React-based block",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "homepage": "https://blockprotocol.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "packages/block-template-react"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "scripts": {
    "build": "block-scripts build",
    "dev": "block-scripts dev",
    "fix:eslint": "eslint --fix .",
    "format": "prettier --write --ignore-unknown .",
    "lint:eslint": "eslint .",
    "lint:tsc": "tsc --noEmit",
    "serve": "block-scripts serve"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.0.18"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@types/react-dom": "^18.0.6",
    "block-scripts": "0.0.17",
    "eslint": "8.24.0",
    "mock-block-dock": "0.0.33",
    "prettier": "2.7.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "Template",
    "examples": [
      {
        "name": "World"
      }
    ],
    "icon": "public/omega.svg",
    "image": "public/block-preview.png",
    "name": "block-template-react",
    "protocol": "0.2"
  }
}
