{
  "name": "@blockprotocol/core",
  "version": "0.0.2",
  "description": "Implementation of the Block Protocol Core specification for blocks and embedding applications",
  "keywords": [
    "blockprotocol",
    "blocks",
    "core"
  ],
  "homepage": "https://blockprotocol.org",
  "bugs": {
    "url": "https://github.com/blockprotocol/blockprotocol/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "packages/@blockprotocol/core"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "yarn clean; yarn build:cjs; yarn build:esm",
    "build:cjs": "tsc --module commonjs --outDir dist/cjs ",
    "build:esm": "tsc",
    "clean": "rimraf ./dist/",
    "lint:tsc": "tsc --noEmit",
    "prepare": "yarn build"
  },
  "dependencies": {
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "rimraf": "^3.0.2",
    "typescript": "4.7.2"
  }
