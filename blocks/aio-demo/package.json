{
  "name": "@blocks/aio-demo",
  "version": "0.0.0",
  "description": "This is an all-in-one demo block for the Block Protocol.",
  "keywords": [
    "blockprotocol",
    "blocks"
  ],
  "homepage": "https://blockprotocol.org",
  "repository": {
    "type": "git",
    "url": "git@github.com:blockprotocol/blockprotocol.git",
    "directory": "libs/mock-block-dock"
  },
  "license": "MIT",
  "author": {
    "name": "HASH",
    "url": "https://hash.ai"
  },
  "type": "module",
  "scripts": {
    "build": "block-scripts build",
    "codegen": "yarn exe ./scripts/codegen.ts",
    "dev": "block-scripts dev",
    "exe": "node --unhandled-rejections=strict --experimental-loader ts-node/esm/transpile-only --es-module-specifier-resolution=node --require=suppress-experimental-warnings",
    "fix:eslint": "eslint --fix .",
    "format": "prettier --write --ignore-unknown .",
    "lint:eslint": "eslint --report-unused-disable-directives .",
    "lint:tsc": "tsc --noEmit",
    "schema": "block-scripts codegen",
    "serve": "block-scripts serve"
  },
  "prettier": {
    "printWidth": 80,
    "trailingComma": "all"
  },
  "dependencies": {
    "@blockprotocol/graph": "0.1.0",
    "@blockprotocol/type-system": "0.1.0",
    "chalk": "5.2.0",
    "fs-extra": "^10.1.0"
  },
  "devDependencies": {
    "@local/eslint-config": "0.0.0-private",
    "@local/tsconfig": "0.0.0-private",
    "@types/react-dom": "^18.0.9",
    "block-scripts": "0.1.0",
    "eslint": "8.33.0",
    "mock-block-dock": "0.1.0",
    "prettier": "2.8.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "typescript": "4.9.4"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "block-scripts": {
    "port": 63212
  },
  "blockprotocol": {
    "blockType": {
      "entryPoint": "react"
    },
    "displayName": "aio-demo",
    "examples": [
      {
        "https://blockprotocol.org/@blockprotocol/types/property-type/name/": "World"
      }
    ],
    "icon": "public/omega.svg",
    "image": "public/block-preview.png",
    "name": "aio-demo",
    "protocol": "0.3",
    "schema": "https://blockprotocol.org/@blockprotocol/types/entity-type/thing/v/2"
  }
}
