{
  "name": "@blockprotocol/site",
  "version": "0.0.0",
  "private": true,
  "license": "MIT",
  "scripts": {
    "build": "yarn build-blocks && yarn generate-blocks-data && yarn generate-sitemap && next build",
    "build-block": "./scripts/build-blocks.sh",
    "build-blocks": "find ../hub -type f | xargs ./scripts/build-blocks.sh",
    "clean": "rimraf ./.next/",
    "dev": "yarn generate-sitemap && yarn generate-blocks-data && next dev",
    "dev:db": "docker-compose -f docker-compose.dev.yml --env-file .env.local up",
    "dev:seed-db": "node --max-old-space-size=2048 node_modules/.bin/ts-node --files --project tsconfig.json scripts/seed-db",
    "generate-blocks-data": "node --max-old-space-size=2048 node_modules/.bin/ts-node --files --project tsconfig.json scripts/generate-blocks-data",
    "generate-sitemap": "node --max-old-space-size=2048 node_modules/.bin/ts-node --files --project tsconfig.json scripts/generate-sitemap",
    "postinstall": "yarn generate-sitemap && yarn generate-blocks-data",
    "lint:tsc": "tsc --noEmit # @blockprotocol/site",
    "seed-db": "node --max-old-space-size=2048 node_modules/.bin/ts-node --files --project tsconfig.json scripts/seed-db",
    "serve:dev-db": "docker-compose -f docker-compose.dev.yml --env-file .env.local up",
    "start": "next start"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.46.0",
    "@emotion/cache": "^11.6.0",
    "@emotion/react": "^11.7.0",
    "@emotion/server": "^11.4.0",
    "@emotion/styled": "^11.6.0",
    "@mui/material": "^5.2.1",
    "@twind/next": "1.0.8",
    "axios": "^0.24.0",
    "blockprotocol": "^0.0.1",
    "clsx": "^1.1.1",
    "connect-mongo": "^4.6.0",
    "cookie-signature": "^1.1.0",
    "cors": "^2.8.5",
    "date-fns": "^2.27.0",
    "dedent": "^0.7.0",
    "dotenv": "^10.0.0",
    "express-session": "^1.17.2",
    "express-validator": "^6.14.0",
    "glob": "^7.2.0",
    "gray-matter": "^4.0.3",
    "gsap": "^3.8.0",
    "html-to-text": "^8.1.0",
    "jsonschema": "^1.4.0",
    "lodash": "4.17.21",
    "md5": "^2.3.0",
    "mongodb": "^4.2.2",
    "next": "11.1.3",
    "next-connect": "^0.11.0",
    "next-fonts": "^1.5.1",
    "next-images": "1.8.1",
    "next-mdx-remote": "^3.0.8",
    "next-session": "^4.0.4",
    "nodemailer": "^6.7.2",
    "passport": "^0.5.2",
    "prismjs": "^1.25.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-slick": "^0.28.1",
    "remark-mdx": "1.6.22",
    "remark-parse": "9.0.0",
    "rword": "^3.2.0",
    "slick-carousel": "^1.8.1",
    "slugify": "^1.6.3",
    "twind": "^0.16.16",
    "unified": "9.2.2",
    "uuid": "^8.3.2"
  },
  "devDependencies": {
    "@babel/runtime": "^7.16.3",
    "@types/cookie-signature": "^1.0.4",
    "@types/cors": "^2.8.12",
    "@types/dedent": "^0.7.0",
    "@types/express-session": "^1.17.4",
    "@types/html-to-text": "^8.0.1",
    "@types/lodash": "4.14.171",
    "@types/md5": "^2.3.1",
    "@types/nodemailer": "^6.4.4",
    "@types/passport": "^1.0.7",
    "@types/prismjs": "^1.16.6",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@types/react-slick": "^0.23.7",
    "@types/uuid": "^8.3.4",
    "babel-plugin-inline-react-svg": "2.0.1",
    "rimraf": "^3.0.2",
    "ts-node": "^10.4.0",
    "typescript": "4.3.5"
  }
}
